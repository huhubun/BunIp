{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "IpTest": {
    // 部署站点信息，通过请求头中的 Host 进行识别
    "DeploySite": {
      // 混合部署，同时支持 IPv4 和 IPv6 访问
      "Hybrid": {
        // 域名（不需要填写 HTTP 协议，例如 ip.bun.plus）
        "Domain": "localhost",
        // 协议（http、https）
        "Scheme": "http",
        // 端口号
        "Port": "5000"
      },
      // IPv4 Only
      "IPv4": {
        "Domain": "127.0.0.1",
        "Scheme": "http",
        "Port": "14444"
      },
      // IPv6 Only
      "IPv6": {
        "Domain": "[::1]",
        "Scheme": "http",
        "Port": "16666"
      }
    }
  }

}
