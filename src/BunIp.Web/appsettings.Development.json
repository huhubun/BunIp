{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "BunIp": {
    // 部署站点信息，通过请求头中的 Host 进行识别
    "DeploySite": {
      // 混合部署，同时支持 IPv4 和 IPv6 访问
      "Hybrid": {
        // 域名（不需要填写 HTTP 协议，例如 ip.bun.plus）
        "Domain": "localhost",
        // 协议（http、https）
        "Scheme": "http",
        // 端口号
        "Port": "19317"
      },
      // IPv4 Only
      "IPv4": {
        "Domain": "ipv4.bun.plus",
        "Scheme": "https"
      },
      // IPv6 Only
      "IPv6": {
        "Domain": "ipv6.bun.plus",
        "Scheme": "https"
      }
    },

    // 广告管理
    // 注：A、B 广告位图片宽度需要 <= 1100px
    "Ad": {
      // IP 结果下方 A 号广告位
      "A": null,
      // 服务器接收的 Headers 列表和 Footer 之间，B 号广告位
      "B": null,
      // Footer 中的文字推广，F 号广告位
      "F": null
    }
  }
}
