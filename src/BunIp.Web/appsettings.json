{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "BunIp": {
    // IPv4|IPv6|Hybrid 部署站点的绑定模式，用于控制站点是否显示 IPv6 图标（只有纯 IPv6 才应该显示图标）
    "DeployMode": "Hybrid",

    // 广告管理
    // 注：A、B 广告位图片宽度需要 <= 1100px
    "Ad": {
      // IP 结果下方 A 号广告位
      "A": {
        "ImageUrl": "",
        "Url": "",
        "Description": "",
        "Enable": ""
      },
      // 服务器接收的 Headers 列表和 Footer 之间，B 号广告位
      "B": {
        "ImageUrl": "",
        "Url": "",
        "Description": "",
        "Enable": ""
      },
      // Footer 中的文字推广，F 号广告位
      "F": {
        "Text": "",
        "Url": "",
        "Enable": ""
      }
    }
  }
}
